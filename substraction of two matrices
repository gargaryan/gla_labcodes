#include<stdio.h>
int main()

{
    int sub[2][3],matrix_1[2][3],matrix_2[2][3],i,j;
    printf("\nenter first matrix  of order 2*3");
    for(i=0;i<2;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&matrix_1[i][j]);

        }
    }
         printf("\nenter the second matrix of order 2*3");
        for(i=0;i<2;i++)
        {
            for(j=0;j<3;j++)
            {
                scanf("%d",&matrix_2[i][j]);

            }
        }

            for(i=0;i<2;i++)
            {
                for(j=0;j<3;j++)
                {
                    sub[i][j]= matrix_1[i][j]-matrix_2[i][j];

                }
            } 
              printf("\n substraction of two matrices");
                for(i=0;i<2;i++)
                {
                    for(j=0;j<3;j++)
                    {
                        printf("\n%d\t",sub[i][j]);

                    }
                    printf("\n");
                }


      return 0;

}

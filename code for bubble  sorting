#include<stdio.h>
int main()
{
    int arr[4]={10,25,5,6};
    int i=0,j=0,temp;
    for(i=0;i<4;i++)
    {
        for(j=0;j<3;j++)
        {
            if(arr[j+1]>arr[j])
            {
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;

            }


        }
    }
    for(i=0;i<4;i++)
    {



        printf("%d ",arr[i]);

    }
return 0;
    }

#include<stdio.h>
int main()
{
    int n,i,f=0;
    printf("enter  a number");
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
            f=1;
            break;

        }

    }   if(f==1)
        {

            printf("number is not prime");
        }
            if(f==0)
            {
                printf("number is prime");
            }
    return 0;

}

#include<stdio.h>
int main()
{
    int n,r,s=0,temp;
    printf("enter a number");
    scanf("%d",&n);
       temp=n;
    while(n>0)
    {
        r=n%10;
        n=n/10;
        s=s*10+r;



    }
    printf("reverse number will be=%d",s);
    if(temp==s)
    {
        printf("number is palindrome");
    }
    else
    {
        printf("number is not palindrome");
    }



}
